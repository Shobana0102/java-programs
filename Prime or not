import java.util.*;
public class Main
{
public static void main(String[] args) {  
   int count=15,count1=0;
   for(int i=1;i<=count;i++)
   {
      int a=count%i;    
      if(a==0)
      {
          count1++;
      }
   }
   if(count1==2)
   {
   System.out.println("prime number");
   }
   else{
       System.out.println("not a prime number");
   }
  }
}

