import java.util.*;
public class Main{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        String reverse="";
        int org=a.length();
        for(int i=org-1;i>=0;i--)
        {
            reverse=reverse+a.charAt(i);
        }
        System.out.println(reverse);
  }
}
